@implements IGenerationButton

<StockButton StockButtonCss="btn btn-success"
             Text="Generate Green"
             StockActionAsync=@(async db => await db.GenerateGreenAsync())
             StockLevels=@StockLevels />

@code {

    [Parameter]
    public Func<StockLevels> StockLevels { get; set; } = () => null;

}
